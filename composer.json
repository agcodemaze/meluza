{
    "name": "meluza/app",
    "description": "Sistema MELUZA - Gerenciamento de diaristas e faxinas",
    "type": "project",
    "require": {
        "aws/aws-sdk-php": "^3.298",
        "firebase/php-jwt": "^6.10",
        "intervention/image": "^2.7",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "bin-dir": "bin/"
    }
}
